{
  "name": "patternfly-java-showcase-j2cl",
  "private": true,
  "parcelIgnore": [
    "showcase.js"
  ],
  "source": [
    "src/web/dev.html",
    "src/web/index.html"
  ],
  "scripts": {
    "watch": "parcel serve src/web/dev.html --https --cert cert.pem --key key.pem --dist-dir target/showcase --open",
    "dev": "parcel build src/web/dev.html --dist-dir target/showcase --no-optimize",
    "prod": "parcel build src/web/index.html --dist-dir target/showcase",
    "http-server": "http-server target/showcase --ssl --cert cert.pem -o"
  },
  "devDependencies": {
    "@parcel/core": "^2.12.0",
    "@parcel/reporter-cli": "^2.12.0",
    "http-server": "^14.1.1",
    "parcel": "^2.12.0",
    "parcel-resolver-ignore": "^2.2.0"
  },
  "dependencies": {
    "@github/relative-time-element": "^4.3.1",
    "@patternfly/patternfly": "^5.2.1",
    "highlight.js": "^11.9.0"
  }
}
